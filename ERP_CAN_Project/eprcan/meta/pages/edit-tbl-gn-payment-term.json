        "name": "canEdit",
        "type": "set",
        "value": "true",
        "valueType": "bool"
      },
      {
        "error": [],
        "name": "CanERP_DB_AT132.getTblGnPaymentTermByPaymentTerm_SEQ",
        "parameters": [
          {
            "name": "PaymentTerm_SEQ",
            "value": "${parameters.PaymentTerm_SEQ}"
          }
        ],
        "then": [
          {
            "name": "tblgnpaymentterm",
            "type": "set",
            "value": "${result}",
            "valueType": "ErpCan.Models.CanErpDbAt132.TblGnPaymentTerm"
          }
        ],
        "type": "invoke"
      }
    ]
