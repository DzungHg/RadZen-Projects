{
  "components": [
    {
      "type": "content",
      "container": "contentContainer0",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Payment Type Id",
                              "component": "PaymentType_ID",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "maxLength": 20,
                              "type": "textbox",
                              "name": "PaymentType_ID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblgnpaymenttype.PaymentType_ID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "PaymentType_IDRequiredValidator",
                              "component": "PaymentType_ID",
                              "text": "PaymentType_ID is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Payment Type Text",
                              "component": "PaymentTypeText",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "PaymentTypeText",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblgnpaymenttype.PaymentTypeText}"
                            },
                            {
                              "type": "required-validator",
                              "name": "PaymentTypeTextRequiredValidator",
                              "component": "PaymentTypeText",
                              "text": "PaymentTypeText is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new TblGnPaymentType!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "CanERP_DB_AT132.createTblGnPaymentType",
                        "parameters": [
                          {
                            "name": "TblGnPaymentType",
                            "value": "${tblgnpaymenttype}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${tblgnpaymenttype}"
                          }
                        ],
                        "type": "invoke",
                        "argType": "ErpCan.Models.CanErpDbAt132.TblGnPaymentType"
                      }
                    ]
                  },
                  "data": "${tblgnpaymenttype}",
                  "visible": "${tblgnpaymenttype != null}",
                  "itemType": "ErpCan.Models.CanErpDbAt132.TblGnPaymentType"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "tblgnpaymenttype",
        "type": "set",
        "value": "new ErpCan.Models.CanErpDbAt132.TblGnPaymentType()",
        "valueType": "ErpCan.Models.CanErpDbAt132.TblGnPaymentType"
      }
    ]
  },
  "name": "Add Tbl Gn Payment Type",
  "layout": "Main"
}