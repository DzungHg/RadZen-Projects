{
  "components": [
    {
      "type": "content",
      "container": "contentContainer0",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Category Id",
                              "component": "Category_ID",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 5,
                              "type": "textbox",
                              "name": "Category_ID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              }
                            },
                            {
                              "type": "required-validator",
                              "name": "Category_IDRequiredValidator",
                              "component": "Category_ID",
                              "text": "Category_ID is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Category Text",
                              "component": "CategoryText",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "CategoryText",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Inactive",
                              "component": "Inactive",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "bool?",
                              "type": "checkbox",
                              "name": "Inactive",
                              "triState": true
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new TblIcCategory!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "CanERP_DB_AT132.createTblIcCategory",
                        "parameters": [
                          {
                            "name": "TblIcCategory",
                            "value": "${tbliccategory}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${tbliccategory}"
                          }
                        ],
                        "type": "invoke",
                        "argType": "ErpCan.Models.CanErpDbAt132.TblIcCategory"
                      }
                    ]
                  },
                  "data": "${tbliccategory}",
                  "visible": "${tbliccategory != null}",
                  "itemType": "ErpCan.Models.CanErpDbAt132.TblIcCategory"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "tbliccategory",
        "type": "set",
        "value": "new ErpCan.Models.CanErpDbAt132.TblIcCategory()",
        "valueType": "ErpCan.Models.CanErpDbAt132.TblIcCategory"
      }
    ]
  },
  "name": "Add Tbl Ic Category",
  "layout": "Main"
}