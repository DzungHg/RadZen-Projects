        "parameters": [
          {
            "name": "SODetail_SEQ",
            "value": "${parameters.SODetail_SEQ}"
          }
        ],
        "then": [
          {
            "name": "tblsoorderdetail",
            "type": "set",
            "value": "${result}",
            "valueType": "TblSoOrderDetail"
          }
        ],
        "error": [
          {
            "name": "canEdit",
            "type": "set",
            "value": "!(result.status == 400)",
            "valueType": "bool"
          }
        ],
        "type": "invoke"
